{
  "name": "react-tpc-foreach",
  "description": "React component for iterating over arrays and objects",
  "author": "Thiago Prado de Carvalho",
  "main": "dist/react-tpc-foreach.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "version": "0.1.10",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "first:publishing": "npm publish --access public",
    "publish:patch": "git add . && git commit -m \"publishing a patch\" && npm version patch && npm publish",
    "publish:minor": "git add . && git commit -m \"publishing a minor\" && npm version minor && npm publish",
    "publish:major": "git add . && git commit -m \"publishing a major\" && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagopcdev/react-tpc-foreach.git"
  },
  "keywords": [
    "react-foreach",
    "foreach",
    "React",
    "TypeScript",
    "frontend"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.0.3"
  }
}
